[loggers]
keys=root

[handlers]
keys=file_handler

[formatters]
keys=formatter

[logger_root]
level=DEBUG
handlers=file_handler

[handler_stream_handler]
class=StreamHandler
formatter=formatter
args=(sys.stderr,)

[handler_file_handler]
class=FileHandler
formatter=formatter
args=('tests.log',)

[formatter_formatter]
format=%(asctime)s [%(levelname)s] %(name)s.%(funcName)s: %(message)s

# vim: filetype=cfg
